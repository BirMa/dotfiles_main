! configuration for xterm (better to use URxvt instead)
Xft.lcdfilter: lcddefault

xterm*font:  -misc-fixed-medium-r-normal--15-*-*-*-*-*-*-1
!XTerm*faceName: monospace:7
!xterm*font:	DejaVu Sans Mono:pixelsize=20
XTerm*background: #000000
XTerm*foreground: #00FF00
XTerm*pointerColor: blue
XTerm*pointerColorBackground: white
XTerm*cursorColor: #00ff00
XTerm*internalBorder: 3
XTerm*loginShell: true
XTerm*scrollBar: false
XTerm*scrollKey: true
XTerm*saveLines: 4000
XTerm*multiClickTime: 250 

XTerm*color0: #2E3436
